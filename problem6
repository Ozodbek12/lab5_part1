#include <iostream>
#include <iomanip>

int main() {
    int num_classes;
    std::cin >> num_classes;
    double total_credits = 0.0;
    double total_points = 0.0;
    for (int i = 0; i < num_classes; ++i) {
        double credits, grade;
        std::cin >> credits >> grade;
        total_credits += credits;
        total_points += credits * grade;
    }
    double gpa = total_points / total_credits;
    std::cout << "Your total GPA is " << std::fixed << std::setprecision(4) << gpa << std::endl;
    return 0;
}
